{
  "name": "dice-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "stop:dev": "powershell.exe -NoProfile -Command \"Get-Process -Id (Get-NetTCPConnection -LocalPort 3000).OwningProcess | Stop-Process\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "zip:context": "bash -lc \"set -e; tree -a -I 'node_modules|.next|.git|dist|build|rollforge-context.zip' -L 5 > project-tree.txt; yarn test --coverage; zip -r rollforge-context.zip . -x 'node_modules/*' '.next/*' '.git/*' 'dist/*' 'build/*' '*.log' 'rollforge-context.zip'\"",
    "zip:context:tar": "bash -lc \"set -e; OUTDIR=..; (command -v tree >/dev/null && tree -a -I 'node_modules|.next|.git|dist|build|rollforge-context.tar.gz' -L 5 || find . -path './node_modules' -prune -o -path './.next' -prune -o -path './.git' -prune -o -path './dist' -prune -o -path './build' -prune -o -print) > $OUTDIR/project-tree.txt; yarn test --coverage; tar --exclude='./node_modules' --exclude='./.next' --exclude='./.git' --exclude='./dist' --exclude='./build' --exclude='./rollforge-context.tar.gz' -czf $OUTDIR/rollforge-context.tar.gz .\"",
    "db:status": "powershell.exe -NoProfile -Command \"(Get-Service -Name 'postgresql-x64-17').Status\"",
    "db:stop": "powershell.exe -NoProfile -Command \"Start-Process PowerShell -Verb RunAs -ArgumentList 'Stop-Service -Name \\\"postgresql-x64-17\\\"'\"",
    "db:start": "powershell.exe -NoProfile -Command \"Start-Process PowerShell -Verb RunAs -ArgumentList 'Start-Service -Name \\\"postgresql-x64-17\\\"'\"",
    "db:restart": "powershell.exe -NoProfile -Command \"Start-Process PowerShell -Verb RunAs -ArgumentList 'Restart-Service -Name \\\"postgresql-x64-17\\\"'\"",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "db:migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "db:migrate:prod": "dotenv -e .env.production -- prisma migrate deploy",
    "db:seed": "dotenv -e .env.local -- ts-node --project tsconfig.seeding.json prisma/seed.ts",
    "studio": "dotenv -e .env.local -- prisma studio"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.13.0",
    "next": "15.4.2",
    "next-auth": "^4.24.11",
    "next-test-api-route-handler": "^5.0.0",
    "prisma": "^6.13.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "three": "^0.179.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@types/three": "^0.179.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.0.5",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
